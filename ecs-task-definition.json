{
  "family": "luckyweeky-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::397064606679:role/luckyweeky-ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "law10000hours/luckyweeky-server:${TAG}",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ]
}
