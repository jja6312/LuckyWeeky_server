{
  "family": "luckyweeky-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::397064606679:role/luckyweeky-ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "law10000hours/luckyweeky-server:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "secrets": [
        {
          "name": "DB_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:DB_URL"
        },
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:DB_USERNAME"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:DB_PASSWORD"
        },
        {
          "name": "AWS_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:AWS_ACCESS_KEY"
        },
        {
          "name": "AWS_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:AWS_SECRET_KEY"
        },
        {
          "name": "BUCKET_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:BUCKET_NAME"
        },
        {
          "name": "MACHINE_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:MACHINE_ID"
        },
        {
          "name": "OPENAI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:OPENAI_API_KEY"
        },
        {
          "name": "GPT_API_ENDPOINT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:GPT_API_ENDPOINT"
        },
        {
          "name": "CLOVA_API_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:CLOVA_API_URL"
        },
        {
          "name": "CLOVA_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:CLOVA_ACCESS_KEY"
        },
        {
          "name": "CLOVA_SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:397064606679:secret:CLOVA_SECRET_KEY"
        }

      ],

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/luckyweeky",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
